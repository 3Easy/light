/* RGB Animation */

@-webkit-keyframes rgb {
	0% {
		background: $green;
	}
	33% {
		background: $red;
	}
	66% {
		background: $blue;
	}
	100% {
		background: $green;
	}
}

/* GYRMBC Animation */

@-webkit-keyframes gyrmbc {
	0% {
		background: $green;
	}
	16.6666% {
		background: $yellow;
	}
